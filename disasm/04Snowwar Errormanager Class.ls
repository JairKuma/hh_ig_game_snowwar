on construct mereturn TRUEendon deconstruct mereturn TRUEendon Refresh me, tTopic, tdataif tdata = FALSE thenreturn FALSEend ifcase tdata[#reason] of"game_deleted":tAlertStr = "gs_error_game_deleted""nocredits":tAlertStr = "gs_error_nocredits"otherwise:tAlertStr = "gs_error_" & tdata[#request] & "_" & tdata[#reason]if not textExists(tAlertStr) then -- jump 17tAlertStr = "gs_error_" & tdata[#reason]end casereturn executeMessage(#alert, [#id: "gs_error", #Msg: tAlertStr]))end